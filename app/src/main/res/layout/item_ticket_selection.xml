<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginBottom="@dimen/margin_10">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="@drawable/bg_a0a0a0_stock_1"
        android:padding="@dimen/margin_5">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Contest Fee"
                        android:textSize="@dimen/_9sdp"
                        android:fontFamily="@font/montserrat_medium"
                        android:gravity="center"
                        android:textColor="@color/color_424242"
                        android:maxLines="1"
                        android:ellipsize="end"/>
                    <TextView
                        android:id="@+id/tvEntryFees"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:text="₹ 1000"
                        android:fontFamily="@font/montserrat_medium"
                        android:textSize="@dimen/_10sdp"
                        android:textColor="@color/colorPrimary"
                        android:gravity="center"
                        android:maxLines="1"
                        android:ellipsize="end"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Players"
                            android:textSize="@dimen/_9sdp"
                            android:gravity="center"
                            android:fontFamily="@font/montserrat_medium"
                            android:textColor="@color/color_424242"
                            android:maxLines="1"
                            android:ellipsize="end"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginStart="3dp"
                            android:background="@color/color_red"
                            android:gravity="center"
                            android:padding="1dp"
                            android:fontFamily="@font/montserrat_medium"
                            android:text="UL"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_7sdp" />
                    </LinearLayout>
                    <TextView
                        android:id="@+id/tvTotalTicket"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:text="500"
                        android:fontFamily="@font/montserrat_medium"
                        android:textSize="@dimen/_10sdp"
                        android:textColor="@color/colorPrimary"
                        android:gravity="center"
                        android:maxLines="1"
                        android:ellipsize="end"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Winnings"
                            android:textSize="@dimen/_9sdp"
                            android:fontFamily="@font/montserrat_medium"
                            android:gravity="center"
                            android:textColor="@color/color_424242"
                            android:maxLines="1"
                            android:ellipsize="end"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="UL"
                            android:textSize="@dimen/_7sdp"
                            android:gravity="center"
                            android:textColor="@color/white"
                            android:fontFamily="@font/montserrat_medium"
                            android:background="@color/color_red"
                            android:padding="1dp"
                            android:layout_marginStart="3dp"/>
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tvTotalWinnings"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:fontFamily="@font/montserrat_medium"
                        android:maxLines="1"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/_10sdp"
                        tools:text="₹ 2000" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Max winners"
                            android:textSize="@dimen/_9sdp"
                            android:fontFamily="@font/montserrat_medium"
                            android:gravity="center"
                            android:textColor="@color/color_424242"
                            android:maxLines="1"
                            android:ellipsize="end"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="UL"
                            android:textSize="@dimen/_7sdp"
                            android:gravity="center"
                            android:textColor="@color/white"
                            android:fontFamily="@font/montserrat_medium"
                            android:background="@color/color_red"
                            android:padding="1dp"
                            android:layout_marginStart="3dp"/>
                    </LinearLayout>
                    <TextView
                        android:id="@+id/tvMaxWinners"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:text="3000"
                        android:textSize="@dimen/_10sdp"
                        android:textColor="@color/colorPrimary"
                        android:fontFamily="@font/montserrat_medium"
                        android:gravity="center"
                        android:maxLines="1"
                        android:ellipsize="end"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_10"
                    android:layout_marginEnd="@dimen/margin_10"
                    android:background="@drawable/bg_rounded_colorprimary_ticket"
                    android:layout_marginTop="@dimen/margin_10"
                    android:layout_marginBottom="@dimen/margin_10"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:paddingStart="@dimen/margin_20"
                    android:paddingEnd="@dimen/margin_20"
                    android:layout_weight="1">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/tvMinAns"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/montserrat_medium"
                            tools:text="100"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_12sdp"/>
                        <TextView
                            android:id="@+id/tvMaxAns"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:text="200"
                            android:fontFamily="@font/montserrat_medium"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_12sdp"
                            android:layout_alignParentEnd="true"/>
                    </RelativeLayout>
                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:layout_marginBottom="@dimen/margin_10">
                      <!--  <com.crystal.crystalrangeseekbar.widgets.CrystalRangeSeekbar
                            android:id="@+id/rangeSeekBar"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:corner_radius="0"
                            app:bar_color="@color/color_login_btn"
                            app:bar_highlight_color="@color/color_login_btn"
                            app:left_thumb_color="@color/color_login_btn"
                            app:left_thumb_color_pressed="@color/color_login_btn"
                            app:right_thumb_color="@color/color_login_btn"
                            app:right_thumb_color_pressed="@color/color_login_btn"
                            app:data_type="_float"
                            app:seek_bar_touch_enabled="false"
                            android:layout_gravity="center"/>
                 -->       <TextView
                            android:id="@+id/tvBracketSize"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                        android:fontFamily="@font/montserrat_medium"
                        tools:text="1000"
                            android:textColor="@color/colorPrimary"
                            android:textSize="@dimen/_12sdp"
                            android:layout_gravity="center"
                            android:gravity="center"/>
                    </FrameLayout>
                </LinearLayout>
                <CheckBox
                    android:id="@+id/chkSelect"
                    android:layout_width="@dimen/height_40"
                    android:layout_height="@dimen/height_40"
                    android:text=""
                    android:textSize="@dimen/fontsize_14"
                    android:fontFamily="@font/montserrat_medium"
                    android:textColor="@color/colorPrimary"
                    android:padding="@dimen/margin_5"
                    android:button="@drawable/select_checkbox_drawable"
                    android:layout_gravity="center"/>
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <FrameLayout
        android:id="@+id/frmSelected"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/transparent_50"
        android:visibility="gone"/>


</FrameLayout>